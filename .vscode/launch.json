	"version": "0.2.0",
	"configurations": [
		{
			"name": "gpt-flask-app",
			"type": "python",
			"request": "launch",
			"cwd": "${workspaceFolder}/gpt-tool",
			"program": "${workspaceFolder}/gpt-tool/app.py",
			"python": "${workspaceFolder}/gpt-tool/.venv/bin/python",
			"console": "integratedTerminal",
			"justMyCode": true
